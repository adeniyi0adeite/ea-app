{
  "name": "back-end",
  "version": "1.0.0",
  "description": "An API for registration, login, and profile management using MySQL, JWT, and Knex.",
  "main": "src/app.ts",
  "scripts": {
    "migrate:latest": "knex migrate:latest --knexfile knexfile.ts",
    "migrate:rollback": "knex migrate:rollback --knexfile knexfile.ts",
    "seed:run": "knex seed:run --knexfile knexfile.ts",
    "start": "ts-node src/app.ts",
    "build": "tsc",
    "dev": "ts-node-dev src/app.ts"
  },
  "dependencies": {
    "@types/knex": "^0.15.2",
    "axios": "^1.7.9",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "mysql": "^2.18.1",
    "mysql2": "^3.12.0"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.3"
  },
  "keywords": [
    "user",
    "api",
    "authentication",
    "registration",
    "profile management"
  ],
  "author": "Adeniyi Adeite",
  "license": "MIT"
}
